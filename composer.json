    "fetched",
    "download",
    "dependency",
    "dependencies",
    "frontend",
    "front-end",
    "front",
    "end",
    "javascript",
    "js",
    "css",
    "scss",
    "sass",
    "style",
    "stylesheet",
    "build",
    "compile",
    "tool",
    "file",
    "local",
    "url",
    "http",
    "https"
  ],
  "homepage": "https://github.com/fivefifteen/fetcher",
  "license": "MIT",
  "bin": [
    "bin/fetcher"
  ],
  "autoload": {
    "psr-4": {
      "Fetcher\\": "src/"
    }
  },
  "require": {
    "php": "^8.1",
    "adhocore/cli": "^1.9",
    "composer/semver": "^3.4",
    "splitbrain/php-archive": "^1.4"
  },
  "require-dev": {
    "mockery/mockery": "^1.6",
    "phpunit/phpunit": "^10.5.62"
  },
  "scripts": {
    "test": "vendor/bin/phpunit tests --display-warnings"
  }
